<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F1F8"
    tools:context=".TechnologyActivity">

    <ImageView
        android:id="@+id/archi_star5"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="104dp"
        app:layout_constraintStart_toEndOf="@+id/archi_star4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/archi_star4"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/archi_star5"
        app:layout_constraintStart_toEndOf="@+id/archi_star3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/archi_star1"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/archi_star2"
        app:layout_constraintStart_toEndOf="@+id/architecture"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/archi_star2"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/archi_star3"
        app:layout_constraintStart_toEndOf="@+id/archi_star1"
